<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping
	package="com.ppc.Ticket_sell.bean"
	default-access="field">

	<class name="ContactsBean" table="tbContacts">
	<id name="contactNo" >
	   <generator class="native"></generator>
	</id>
	<property name="contactName"></property>
	<property name="contactId"></property>
	<many-to-one name="user" column="userName" cascade="none" lazy="false"></many-to-one>
	</class>
    
    
     
</hibernate-mapping>
